<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Version>5.4.0-pre1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>True</IncludeSymbols>
    <IncludeSource>True</IncludeSource>
    <PackageProjectUrl>https://github.com/threax/HtmlRapier.TagHelpers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/threax/HtmlRapier.TagHelpers</RepositoryUrl>
    <Authors>Andrew Piper</Authors>
    <PackageLicenseUrl>https://github.com/threax/HtmlRapier.TagHelpers/blob/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>Added an attribute for client config that will expand the path if it starts with ~/ when being written.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.0.1" />
  </ItemGroup>
  
</Project>